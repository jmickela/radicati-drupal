{% embed "@atoms/10-link/_link.twig" with {
    link: {
        mods: icon_link.mods,
        base: icon_link.base|default('iconlink'),
        href: icon_link.href,
        attributes: icon_link.link_attributes
    },
    icon: {
        slug: icon_link.slug
    }
} only %}
    {% block link_content %}
        {% include "@atoms/15-icon/_icon.twig" with {
            icon: icon
        } only %}
    {% endblock %}
{% endembed %}
